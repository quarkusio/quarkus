{
  "plugins": [
    {
      "plugin": "./maven-plugin.ts",
      "options": {
        "mavenExecutable": "mvn"
      }
    }
  ],
  "pluginsConfig": {
    "@nx/js": {
      "analyzeSourceFiles": false,
      "analyzePackageJson": false,
      "analyzeLockfile": false
    }
  },
  "workspaceLayout": {
    "projectNameAndRootFormat": "as-provided"
  },
  "$schema": "./node_modules/nx/schemas/nx-schema.json"
}
