{
  "request": {
    "method": "GET",
    "urlPattern": "/base/a-bootiful-client/prod",
    "basicAuth" : {
      "username" : "user",
      "password" : "pass"
    },
    "headers" : {
      "Accept" : {
        "contains" : "application/json"
      },
      "h1": {
        "equalTo": "v1"
      },
      "h2": {
        "equalTo": "v2"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "bodyFileName": "a-bootiful-client-prod.json"
  }
}
